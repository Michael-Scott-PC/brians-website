{
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "code-runner.executorMapByGlob": {
    "*.js": "node"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  // Enable per-language
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  }
}
